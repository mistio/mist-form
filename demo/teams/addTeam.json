{
  "type": "object",
  "id": "add-team-form",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "label": "Add Team",
  "description": "",
  "submitButtonLabel": "Add",
  "canClose": true,
  "additionalProperties": false,
  "properties": {
    "name": {
      "label": "Name",
      "id": "name",
      "type": "string",
      "alwaysFloatLabel": true,
      "required": true
    },
    "description": {
      "label": "Description (optional)",
      "id": "description",
      "type": "string"
    },
    "visible": {
      "label": "Visible to members not in the team.",
      "id": "visible",
      "type": "boolean",
      "noLabelFloat": true
    }
  }
}
